  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist",
    "lib",
    "es",
    "icons",
    "LEGAL.md",
    "assets.json"
