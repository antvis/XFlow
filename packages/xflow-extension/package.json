{
  "private": false,
  "name": "@antv/xflow-extension",
  "version": "1.0.51",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist es lib",
    "start": "pnpm build:watch",
    "build:esm": "tsc --module es6 --target es6 --outDir ./es",
    "build:cjs": "tsc --module commonjs --target es5 --outDir ./lib",
    "build:umd": "rollup -c",
    "build:watch": "pnpm build:esm --w",
    "build:watch:esm": "pnpm build:esm -- --w",
    "build:watch:cjs": "pnpm build:cjs -- --w",
    "build:ts": "run-p build:cjs build:esm build:umd",
    "build:less": "node ./scripts/less.js",
    "build": "run-s build:ts build:less",
    "prebuild": "run-s lint clean",
    "prepare": "pnpm build",
    "release": "pnpm publish --  --access=public",
    "lint": "run-s lint:ts lint:style",
    "lint:ts": "eslint 'src/**/*.{js,ts}?(x)' --fix",
    "lint:style": "stylelint 'src/**/*.less' --syntax less --fix",
    "lint:prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "jest --verbose true",
    "test:coverage": "jest --coverage"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "unpkg": "dist/index.umd.js",
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "@antv/xflow-core": "workspace:*",
    "@antv/xflow-hook": "workspace:*",
    "mana-syringe": "^0.2.2",
    "moment": "^2.29.1",
    "rc-field-form": "^1.22.0",
    "react-color": "2.17.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "react": "^16.8.0  || ^17.0.0",
    "react-dom": "^16.8.0  || ^17.0.0",
    "antd": "^4.6.3",
    "@antv/x6": "^1.30.1",
    "@antv/x6-react-components": "^1.1.15",
    "@antv/x6-react-shape": "^1.5.2",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "classnames": "^2.3.1",
    "fs-extra": "^10.0.0",
    "less": "^4.1.2",
    "less-plugin-npm-import": "^2.1.0",
    "lint-staged": "^11.2.6",
    "lodash": "^4.17.20",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.61.1",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "@ant-design/icons": "^4.6.0",
    "@antv/x6": "^1.30.1",
    "@antv/x6-react-components": "^1.1.15",
    "@antv/x6-react-shape": "^1.2.5",
    "antd": "^4.6.3",
    "classnames": "^2.2.6",
    "react": "^16.8.0  || ^17.0.0",
    "react-dom": "^16.8.0  || ^17.0.0",
    "reflect-metadata": "^0.1.13"
  }
}