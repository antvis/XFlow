{
  "private": false,
  "name": "@antv/xflow-extension",
  "version": "1.0.6",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist es lib",
    "start": "yarn build:watch",
    "build:esm": "tsc --module es6 --target es6 --outDir ./es",
    "build:cjs": "tsc --module commonjs --target es5 --outDir ./lib",
    "build:umd": "rollup -c",
    "build:watch": "yarn build:esm --w",
    "build:watch:esm": "yarn build:esm --w",
    "build:watch:cjs": "yarn build:cjs --w",
    "build:ts": "run-p build:cjs build:esm build:umd",
    "build:less": "node ./scripts/less.js",
    "build": "run-s build:ts build:less",
    "prebuild": "run-s lint clean",
    "prepare": "yarn build",
    "release": "npm publish --access=public",
    "lint": "run-s lint:ts lint:style",
    "lint:ts": "eslint 'src/**/*.{js,ts}?(x)' --fix",
    "lint:style": "stylelint 'src/**/*.less' --syntax less --fix",
    "lint:prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "jest --verbose true",
    "test:coverage": "jest --coverage"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "unpkg": "dist/index.umd.js",
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "@antv/xflow-core": "1.0.6",
    "@antv/xflow-hook": "1.0.6",
    "schemant-form": "1.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.2.5",
    "less": "^4.1.1",
    "less-plugin-npm-import": "^2.1.0",
    "lint-staged": "^11.1.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.6",
    "rimraf": "^3.0.2",
    "rollup": "^2.56.3",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.4.3",
    "@antv/x6": "^1.12.11",
    "@antv/x6-react-shape": "^1.2.5",
    "@antv/x6-react-components": "^1.1.15"
  },
  "peerDependencies": {
    "@ant-design/icons": "^4.6.0",
    "antd": "^4.6.3",
    "classnames": "^2.2.6",
    "lodash": "^4.17.20",
    "react": "^16.8.0  || ^17.0.0",
    "react-dom": "^16.8.0  || ^17.0.0",
    "reflect-metadata": "^0.1.13",
    "@antv/x6": "^1.12.11",
    "@antv/x6-react-shape": "^1.2.5",
    "@antv/x6-react-components": "^1.1.15"
  }
}