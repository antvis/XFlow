{
  "private": true,
  "name": "xflow-quick-start-umi",
  "version": "0.1.33",
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.31.7",
    "@antv/xflow": "workspace:*",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "umi": "^3.5.20"
  },
  "devDependencies": {
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@umijs/babel-preset-umi": "^3.5.20",
    "@umijs/preset-react": "~1.8.30",
    "@umijs/test": "^3.5.20",
    "jquery": "^3.6.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4",
    "yorkie": "^2.0.0"
  },
  "pnpm": {
    "packageExtensions": {
      "jquery-mousewheel": {
        "dependencies": {
          "jquery": "*"
        }
      }
    }
  }
}