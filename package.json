{
  "private": true,
  "name": "xflow-monorepo",
  "version": "1.0.10",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "npm run dev:docs",
    "reset": "pnpm --force --filter @antv/xflow... install",
    "dev:main": "pnpm --filter @antv/xflow start",
    "dev:hook": "pnpm start --filter @antv/xflow-hook",
    "dev:core": "pnpm --filter @antv/xflow-core start",
    "dev:ext": "pnpm --filter @antv/xflow-extension start",
    "dev:docs": "pnpm --filter xflow-docs start",
    "build": "pnpm run build --filter @antv/xflow... --stream",
    "build:less": "pnpm run build:less --filter @antv/xflow... --stream",
    "release": "pnpm publish --no-git-checks -r  --filter @antv/xflow...",
    "ci:publish": "pnpm run release --filter @antv/xflow... --stream",
    "ci:version": "pnpm changeset version",
    "add:changelog": "pnpm changeset",
    "deploy:docs": "pnpm run deploy --filter xflow-docs",
    "test": "pnpm run test --filter @antv/xflow-hook --filter @antv/xflow-core",
    "ci": "echo 'no-ci'",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\""
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "pnpm": {
    "overrides": {
      "colors": "1.2.5",
      "sharp": "^0.29.0"
    }
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "antd": "^4.17.4"
  },
  "devDependencies": {
    "@changesets/cli": "^2.19.0",
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@umijs/fabric": "^2.8.1",
    "eslint": "^7.32.0",
    "eslint-plugin-deprecation": "^1.3.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-no-null": "^1.0.2",
    "lint-staged": "^10.5.4",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4",
    "yorkie": "^2.0.0"
  },
  "ci": {
    "verison": 12
  },
  "repository": "https://github.com/antvis/XFlow.git",
  "author": "boyu.zlj <boyu.zlj@antgroup.com>",
  "license": "MIT"
}