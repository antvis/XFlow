{
  "private": true,
  "name": "xflow",
  "version": "1.0.10",
  "scripts": {
    "start": "npm run dev:docs",
    "reset": "pnpm up --filter @antv/xflow...",
    "dev:main": "cd ./packages/xflow && pnpm start",
    "dev:core": "cd ./packages/xflow-core && pnpm start",
    "dev:ext": "cd ./packages/xflow-extension && pnpm start",
    "dev:docs": "cd ./packages/xflow-docs && pnpm start",
    "build": "pnpm run build --filter @antv/xflow... --stream",
    "build:less": "pnpm run build:less --filter @antv/xflow... --stream",
    "build:pkg": "pnpm run build --filter @antv/xflow... --stream",
    "release:pkg": "pnpm run release --filter @antv/xflow... --stream",
    "deploy:docs": "pnpm run deploy --filter @antv/xflow-docs",
    "ci": "echo 'no-ci'",
    "test": "pnpm run test --filter @antv/xflow-hook --filter @antv/xflow-core",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\""
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "antd": "^4.17.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@types/jest": "^27.0.3",
    "@types/jquery": "^3.5.10",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@umijs/fabric": "^2.8.1",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-named-asset-import": "^0.3.8",
    "babel-preset-react-app": "^10.0.1",
    "dumi": "^1.1.37",
    "eslint": "^7.32.0",
    "eslint-plugin-deprecation": "^1.3.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-no-null": "^1.0.2",
    "gh-pages": "^3.2.3",
    "jest": "^27.4.5",
    "lint-staged": "^10.5.4",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "react-dev-utils": "^11.0.4",
    "rimraf": "^2.7.1",
    "typescript": "^4.5.4",
    "yorkie": "^2.0.0"
  },
  "pnpm": {
    "overrides": {
      "jest": "^27.2.4",
      "@jest/transform": "^27.0.0",
      "@antv/x6-react-components": "^1.1.15",
      "react-color": "2.17.1",
      "@types/react-dom": "^16.8.0  || ^17.0.0",
      "@umijs/preset-dumi": "^1.1.33-0"
    }
  },
  "ci": {
    "verison": 12
  },
  "repository": "https://github.com/antvis/XFlow.git",
  "author": "boyu.zlj <boyu.zlj@antgroup.com>",
  "license": "MIT"
}